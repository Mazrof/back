{
  "name": "back",
  "version": "1.0.0",
  "description": "Yes, separating database queries into a *services* folder is a common practice. This structure further decouples the *business logic* from the *data access layer*, which can lead to a cleaner, more maintainable codebase.",
  "main": "index.js",
  "scripts": {
    "start:build": "tsc -w",
    "check": "tsc --noEmit",
    "build": "tsc",
    "start:run": "nodemon build/server.js",
    "start-dev": "concurrently npm:start:*",
    "migrate-db": "npx prisma migrate dev --name add_status_to_members",
    "prisma-studio": "npx prisma studio",
    "kill-port-3000": "lsof -ti :3000 | xargs kill -9 || true",
    "pull_db": "npx prisma db pull",
    "start": "node build/server.js",
    "container": "npm run migrate-db & npm run start"
  },
